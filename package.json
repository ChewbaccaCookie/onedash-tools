{
  "name": "@onedash/tools",
  "version": "2.3.18",
  "description": "Official OneDash. tools for React",
  "author": "ChewbaccaCookie",
  "license": "UNLICENSED",
  "repository": "ChewbaccaCookie/npm-onedash-tools",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "lint": "npx eslint src/*",
    "start": "rollup -c -w",
    "deploy": "npx del-cli -f dist && npm run build && npx auto-bump && npm publish --registry https://npm.onedash.dev && npm run build-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o public && npx now"
  },
  "dependencies": {
    "@types/cleave.js": "^1.4.1",
    "@types/react-tag-autocomplete": "^5.6.1",
    "@y0c/react-datepicker": "^1.0.1",
    "cleave.js": "^1.5.3",
    "react-select": "^3.0.8",
    "react-sortable-hoc": "^1.10.1",
    "react-tag-autocomplete": "^5.11.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-info": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addon-storysource": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@svgr/rollup": "4.3.3",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "auto-bump": "0.0.10",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-runtime": "6.26.0",
    "cross-env": "6.0.3",
    "dayjs": "1.8.17",
    "del-cli": "^3.0.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "gh-pages": "2.1.1",
    "node-sass": "4.13.0",
    "now": "^16.7.0",
    "react": "16.12.0",
    "react-calendar": "2.19.2",
    "react-docgen-typescript-loader": "3.6.0",
    "react-dom": "16.12.0",
    "react-scripts-ts": "3.1.0",
    "rollup": "1.27.9",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-typescript2": "0.25.2",
    "rollup-plugin-url": "3.0.1",
    "sass-loader": "8.0.0",
    "typescript": "3.7.2"
  },
  "files": [
    "dist"
  ],
  "autoBump": {},
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ]
